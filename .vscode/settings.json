{
    "debug.allowBreakpointsEverywhere": true,
    "debug.hideActionBar": false,
    "html.format.enable": true,
    "html.suggest.html5": true,
    "html.validate.scripts": true,
    "html.validate.styles": true,
    "html.autoClosingTags": true,
    "json.format.enable": true,
    "css.validate": true,
    "less.validate": true,
    "problems.decorations.enabled": true,
    "problems.autoReveal": true,
    "prettier.parser": "flow",
    "prettier.trailingComma": "all",
    "npm-intellisense.scanDevDependencies": true,
    "npm-intellisense.importES6": true,
    "jshint.enable": true,
    "hlsl.suggest.basic": true,
    "vsicons.dontShowNewVersionMessage": true,
    "prettier.javascriptEnable": ["javascript", "javascriptreact"],
    "prettier.cssEnable": ["css", "less", "scss"],
    "prettier.jsonEnable": ["json"],
    "path-intellisense.extensionOnImport": true,
    "npm.autoDetect": "on",
    "npm.packageManager": "npm",
    "beautify.language": {
        "js": {
            "type": ["javascript", "json"],
            "filename": [".jshintrc", ".jsbeautify"]
        },
        "css": ["css", "scss"],
        "html": ["htm", "html"]
    },
    "html.format.contentUnformatted": "pre,code,textarea",
    "editor.autoClosingBrackets": true,
    "editor.matchBrackets": true,
    "bracketPairColorizer.contextualParsing": true,
    "bracketPairColorizer.consecutivePairColors": ["()", "[]", "{}", ["Gold", "Orchid", "LightSkyBlue"], "Red"],
    "explorer.decorations.colors": true,
    "colorize.languages": ["css", "sass", "scss", "less", "postcss", "sss", "stylus", "xml", "svg"],
    "colorize.activate_variables_support_beta": true,
    "editor.tokenColorCustomizations": {
        "variables": "#ABEBC6"
    },
    "javascript.nameSuggestions": true,
    "javascript.referencesCodeLens.enabled": true,
    "javascript.validate.enable": true,
    "html.trace.server": "verbose",
    "json.trace.server": "verbose",
    "typescript.tsserver.trace": "off",
    "css.trace.server": "verbose",
    "liveServer.settings.ChromeDebuggingAttachment": true,
    "liveServer.settings.NoBrowser": true,
    "prettier.useTabs": true,
    "eslint.enable": true,
    "prettier.jsxBracketSameLine": true,
    "prettier.singleQuote": true,
    "emmet.includeLanguages": {
        "HTML": "HTML5",
        "javascript": "javascript"
    },

    "eslint.packageManager": "npm",
    "eslint.nodePath": "/node_modules/eslint",
    "git.ignoreLimitWarning": true,
    "eslint.trace.server": "verbose",
    "eslint._legacyModuleResolve": true,
    "eslint.alwaysShowStatus": true,
    "eslint.options": {
        "configfile": "/.eslintrc.json"
    },
    "eslint.run": "onSave",
    "search-node-modules.useLastFolder": true,
    "search-node-modules.path": "node_modules",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "editor.minimap.enabled": true,
    "editor.smoothScrolling": true,
    "editor.minimap.showSlider": "always",
    "editor.find.autoFindInSelection": true,
    "editor.parameterHints": true,
    "editor.suggestOnTriggerCharacters": true,
    "editor.cursorBlinking": "phase",
    "editor.codeLens": true,
    "editor.dragAndDrop": true,
    "workbench.tips.enabled": true,
    "workbench.editor.showIcons": true,
    "workbench.activityBar.visible": true,
    "files.encoding": "utf8",
    "files.autoSave": "onWindowChange",
    "git.enabled": true,

    "al.editorServicesHost": "bin/Microsoft.Dynamics.Nav.EditorServices.Host.exe",
    "al.enableCodeAnalysis": true,
    "css.fileExtensions": ["css", "scss"],
    "liveServer.settings.donotVerifyTags": false,
    "liveServer.settings.proxy": {
        "enable": false,
        "baseUri": "/",
        "proxyUri": "http://127.0.0.1:8080"
    }
}