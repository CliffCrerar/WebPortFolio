{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:5500/index.html",
            "webRoot": "${workspaceFolder}/WebPortFolio1"
        },
        {
            "name": "Launch Edge",
            "type": "edge",
            "request": "launch",
            "url": "http://localhost:5500/index.html",
            "webRoot": "${workspaceRoot}/WebPortFolio1",
            "sourceMaps": true
        },
        {
            "name": "Firefox-Debug",
            "type": "firefox",
            "request": "launch",
            "runtimeExecutable": "/Applications/FirefoxDeveloperEdition.app/Contents/MacOS/firefox",
            "port": 5500,
            "program": "${workspaceRoot}/index.html",
            "profileDir": "${workspaceRoot}/.firefoxProfile",
            "stopOnEntry": false
        },
        {
            "name": "Firefox-Debug-HTTP",
            "type": "firefox",
            "request": "launch",
            "runtimeExecutable": "/Applications/FirefoxDeveloperEdition.app/Contents/MacOS/firefox",
            "port": 6001,
            "program": "http://localhost:5500/index.html",
            "profileDir": "${workspaceRoot}/.firefoxProfile",
            "stopOnEntry": false
        }
    ]
}